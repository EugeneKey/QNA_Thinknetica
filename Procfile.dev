web: bin/rails server -b $IP_APP_DEV -p 3000
private_pub: bundle exec thin -R private_pub.ru -p 9292 -a $IP_APP_DEV -P tmp/pids/thin_private_pub.pid -l log/thin_private_pub.log start
private_pub_test: rackup private_pub.ru -s thin -E production -p 9293
sidekiq: redis-cli flushdb && bundle exec sidekiq -q default -q mailers
sphinx: bundle exec rake thinking_sphinx:run_in_foreground RAILS_ENV=development
